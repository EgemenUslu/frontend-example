{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "semi": [2, "never"],
    "no-console": 1,
    "object-curly-newline": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "parser": "babel-eslint"
}